{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${mod_group_id}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "${mod_authors}": "Owner"
      },
      "contact": {
        "homepage": "${mod_display_url}"
      },
      "icon": "pack.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_resource_loader",
        "versions": "*",
        "unless": "fabric-resource-loader-v0"
      }
    ]
  }
}